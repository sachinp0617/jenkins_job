pipeline {
    
    agent any

    triggers {
        pollSCM('H * * * *')
    }

    stages {

        stage ('Build') {
                steps{
                       sh 'pwd'
                       sh 'exit 1'
                   }
        }

          stage ('Test') {
                 steps {
                          sh 'pwd'
                          echo "this is test"
                     }
        }
             stage ('Test') {
                steps {
                        sh 'pwd'
                         echo "this is test"
                    }
        }
    }



























}